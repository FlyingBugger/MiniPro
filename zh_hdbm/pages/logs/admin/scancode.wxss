}
.success>view icon{
  margin-left: 40%;
}
.success>view view{
  text-align: center;
  font-size: 30rpx;
  width: 100%;
  border-radius: 10rpx;
  margin: 80rpx auto;
  height: 80rpx;
  color: #fff;
  background: #43B1FF;
  line-height: 80rpx;
