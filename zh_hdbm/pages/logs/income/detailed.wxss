/* zh_hdbm/pages/logs/income/detailed.wxss */
page{
  background: #e5e5e5;
  font-family: '微软雅黑'
}
.container{
  width: 100%;
  height: 100%;
  /* background: #fff; */
}
.detailed{
  width: 95%;
  height: 60rpx;
  background: #fff;
  margin: 10rpx auto;
}
.detailed>view{
  width: 50%;
  height: 100%;
  display: inline-block;
  font-size: 26rpx;
  line-height: 60rpx;
  color: #333;
}
.detailed>view:nth-child(1){
  text-align: left;
}
.detailed>view:nth-child(2){
  text-align: right;
}
.detaileds{
  width: 95%;
  height: 60rpx;
  background: #fff;
  margin: 10rpx auto;
}
.detaileds>view{
  width: 33.3%;
  height: 100%;
  display: inline-block;
  font-size: 26rpx;
  line-height: 60rpx;
  color: #333;
  overflow: hidden;
}
.detaileds>view:nth-child(1){
  text-align: left;
}
.detaileds>view:nth-child(2){
  text-align: center;
}
.detaileds>view:nth-child(3){
  text-align: right;
}