}
.header button{

  line-height: 70rpx;
  font-size: 26rpx;
  color: #fff;
}
.header button::after{
  border: none;
}
.activity_prompt button{
  line-height: 50rpx;
  width: 100%;
  font-size: 26rpx;
  text-align: left;
  background: none;
  padding: 0;
  color: #999;
}
.activity_prompt button::after{
  border: none;
}
.header>view button:nth-child(2) {
  float: right;
   width: 50%;
  text-align: right;
  background: none;
  height: 100%;
}
.add_title {
