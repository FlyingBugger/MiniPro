}

.container {
  width: 100%;
  height: 100%;
}

/*上传图片  */

.upload {
  width: 100%;
  height: 250rpx;
  background: #fff;
  margin-bottom: 10rpx;
  overflow: hidden;
}

.upload image {
  width: 100%;
}

.upload_title {
  width: 90%;
  height: 70rpx;
  font-size: 24rpx;
  color: #333;
  margin: 0 auto;
  line-height: 70rpx;
}

.replace {
  width: 150rpx;
  height: 60rpx;
  position: absolute;
  bottom: 20rpx;
  right: 40rpx;
  font-size: 24rpx;
  text-align: center;
  line-height: 60rpx;
  border-radius: 15rpx;
  overflow: hidden;
  color: #fff;
}

.replace view {
  width: 100%;
  height: 100%;
}

.replace view:nth-child(1) {
  position: absolute;
  top: 0;
  background: #000;
  opacity: 0.4;
  border-radius: 10rpx;
  z-index: 1;
}

.replace view:nth-child(2) {
  position: absolute;
  top: 0;
  z-index: 99;
}

.upload_img {
  width: 150rpx;
  height: 150rpx;
  border: 1rpx solid #ccc;
  margin-left: 5%;
  border-radius: 15rpx;
}

.upload_img>view {
  width: 100rpx;
  height: 3rpx;
  background: #ccc;
}

.upload_img view:nth-child(1) {
  transform: rotate(90deg);
}

/*活动主体  */

.activity {
  width: 100%;
  height: auto;
  background: #fff;
}

.activity_theme {
  width: 95%;
  height: 80rpx;
}

.activity_theme>view {
  width: 100%;
  height: 50rpx;
}

/*icon  */

.activity_img {
  width: 50rpx;
  height: 100%;
  float: left;
  /* overflow: hidden; */
  border-radius: 50%;
  position: relative;
}
.activity_img1 {
  width: 50rpx;
  height: 100%;
  float: right;
  /* overflow: hidden; */
  border-radius: 50%;
  position: relative;
}
.activity_img1 image {
  width: 30%;
  height: 30%;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
}
.activity_img image {
  width: 80%;
  height: 80%;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
}

/*活动名字  */

.activity_text {
  width: 140rpx;
  height: 100%;
  float: left;
  color: #666;
  line-height: 50rpx;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

/*活动提示  */

.choice {
  width: 200rpx;
  height: 100%;
  line-height: 50rpx;
  float: left;
  /* color: #999; */
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.activity_prompt {
  width: 400rpx;
  height: 100%;
  line-height: 50rpx;
  float: left;
  /* color: #999; */
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.activity_prompt input {
  font-family: '微软雅黑';
}

.activity_pro {
  width: 100rpx;
  height: 100%;
  float: right;
}

.switch {
  width: 100rpx;
  height: 60rpx;
  right: 0;
}

.activity_sign {
  width: 300rpx;
  float: left;
  text-indent: 18rpx;
}

.time {
  width: 90%;
  height: 80rpx;
  border-top: 1rpx solid #e5e5e5;
}

.time>picker {
  width: 50%;
  height: 60rpx;
  font-size: 26rpx;
  line-height: 60rpx;
}

.time>picker:nth-child(1) {
  left: 0;
}

.time>picker:nth-child(2) {
  right: 0;
}

.time>picker view {
  width: 100%;
  height: 60rpx;
}

.time input {
  width: 100%;
  height: 100%;
  /* background: red; */
  line-height: 80rpx;
}

.activity_delete {
  width: 100rpx;
  height: 100%;
  font-size: 26rpx;
  color: red;
  line-height: 60rpx;
  float: right;
}

/*活动类型  */

.activity_type {
  width: 90%;
  height: 80rpx;
  line-height: 80rpx;
  margin-left: 5%;
}

.activity_type text {
  color: #41B0FF;
}

.activity_custom {
  width: 100%;
  height: 80rpx;
  background: #fff;
  line-height: 80rpx;
  color: #41B0FF;
  text-indent: 5%;
}

.button {
  width: 90%;
  height: 80rpx;
  line-height: 80rpx;
  background: #43B1FF;
  color: #fff;
  margin-bottom: 30rpx;
}

/*添加自定义填写项  */

