}

.dots {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 20rpx;
  display: flex;
  background: #000;
  width: 120rpx;
  margin: auto;
  border-radius: 10rpx;
  height: 20rpx;
  justify-content: center;
}

.dot {
  margin: 4rpx 8rpx;
  width: 14rpx;
  height: 14rpx;
  background: #fff;
  border-radius: 8rpx;
  transition: all 0.6s;
}

.active {
  width: 34rpx;
